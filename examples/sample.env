# Environment Variables Example
# This file demonstrates how Camouflage hides values in .env files

# API Keys
API_KEY=sk-1234567890abcdef
SECRET_KEY=super_secret_value_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/mydb
DB_HOST=localhost
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=very_secure_password

# Authentication
JWT_SECRET=my-jwt-secret-key-here
AUTH_TOKEN=bearer_token_12345

# External Services
STRIPE_API_KEY=sk_live_abcdef123456
SENDGRID_API_KEY=SG.xxxxxxxxxxxx
AWS_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE
AWS_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY

# Public Values (can be excluded from hiding)
PUBLIC_URL=https://example.com
NODE_ENV=development
DEBUG=true

